{
  "name": "api",
  "root": "apps/api",
  "sourceRoot": "apps/api",
  "projectType": "application",
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "venv\\Scripts\\python manage.py runserver --noreload",
        "cwd": "apps/api"
      }
    },
    "migrate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "python manage.py migrate",
        "cwd": "apps/api"
      }
    }
  }
}
