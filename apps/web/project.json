{
  "name": "web",
  "root": "apps/web",
  "sourceRoot": "apps/web/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/next:build",
      "options": {
        "root": "apps/web",
        "outputPath": "dist/apps/web",
        "nextConfig": "apps/web/next.config.js"
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "options": {
        "buildTarget": "web:build"
      }
    }
  }
}
